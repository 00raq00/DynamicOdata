rem Remove all subassemblies and pdbs
del *.* /q
rem Rename the combined .dll and .pdb to the original project name we started with.
MOVE "$(TargetDir)output\*.*" "$(TargetDir)"
ren "$(TargetDir)$(TargetName).all.pdb" "$(TargetName).pdb"
ren "$(TargetDir)$(TargetName).all.dll" "$(TargetName).dll"
ren "$(TargetDir)$(TargetName).all.xml" "$(TargetName).xml"
exit 0</PostBuildEvent>
